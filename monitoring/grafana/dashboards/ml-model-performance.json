{
  "dashboard": {
    "id": null,
    "title": "Trading Bot - ML Model Performance",
    "tags": ["ml", "models", "prediction", "accuracy"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Model Accuracy Metrics",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "trading_bot_ml_model_accuracy{model=\"trend_prediction\"}",
            "legendFormat": "Trend Prediction Accuracy",
            "refId": "A"
          },
          {
            "expr": "trading_bot_ml_model_accuracy{model=\"price_prediction\"}",
            "legendFormat": "Price Prediction Accuracy",
            "refId": "B"
          },
          {
            "expr": "trading_bot_ml_model_accuracy{model=\"volatility_prediction\"}",
            "legendFormat": "Volatility Prediction Accuracy",
            "refId": "C"
          },
          {
            "expr": "trading_bot_ml_model_accuracy{model=\"sentiment_analysis\"}",
            "legendFormat": "Sentiment Analysis Accuracy",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.7},
                {"color": "dark-green", "value": 0.85}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 2,
        "title": "Model Prediction Confidence",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "targets": [
          {
            "expr": "trading_bot_ml_prediction_confidence{model=\"trend_prediction\"}",
            "legendFormat": "Trend Prediction",
            "refId": "A"
          },
          {
            "expr": "trading_bot_ml_prediction_confidence{model=\"price_prediction\"}",
            "legendFormat": "Price Prediction",
            "refId": "B"
          },
          {
            "expr": "trading_bot_ml_prediction_confidence{model=\"volatility_prediction\"}",
            "legendFormat": "Volatility Prediction",
            "refId": "C"
          }
        ]
      },
      {
        "id": 3,
        "title": "Model Training Loss",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "targets": [
          {
            "expr": "trading_bot_ml_training_loss{model=\"trend_prediction\"}",
            "legendFormat": "Trend Model Loss",
            "refId": "A"
          },
          {
            "expr": "trading_bot_ml_training_loss{model=\"price_prediction\"}",
            "legendFormat": "Price Model Loss",
            "refId": "B"
          },
          {
            "expr": "trading_bot_ml_training_loss{model=\"volatility_prediction\"}",
            "legendFormat": "Volatility Model Loss",
            "refId": "C"
          }
        ]
      },
      {
        "id": 4,
        "title": "Feature Importance",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "targets": [
          {
            "expr": "trading_bot_ml_feature_importance",
            "legendFormat": "{{feature_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "continuous-GrYlRd"},
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 5,
        "title": "Model Inference Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "targets": [
          {
            "expr": "rate(trading_bot_ml_inference_duration_seconds_sum[5m]) / rate(trading_bot_ml_inference_duration_seconds_count[5m])",
            "legendFormat": "{{model}} Inference Time",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Data Drift Detection",
        "type": "stat",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 22},
        "targets": [
          {
            "expr": "trading_bot_ml_data_drift_score",
            "legendFormat": "{{feature}} Drift Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.3},
                {"color": "orange", "value": 0.6},
                {"color": "red", "value": 0.8}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Model Predictions vs Actual",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 22},
        "targets": [
          {
            "expr": "trading_bot_ml_predicted_value{model=\"price_prediction\"}",
            "legendFormat": "Predicted Price",
            "refId": "A"
          },
          {
            "expr": "trading_bot_ml_actual_value{model=\"price_prediction\"}",
            "legendFormat": "Actual Price",
            "refId": "B"
          }
        ]
      },
      {
        "id": 8,
        "title": "Model Usage Statistics",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(trading_bot_ml_predictions_total[5m])",
            "legendFormat": "Predictions/min",
            "refId": "A"
          },
          {
            "expr": "trading_bot_ml_model_version",
            "legendFormat": "Model Version",
            "refId": "B"
          },
          {
            "expr": "time() - trading_bot_ml_last_training_timestamp",
            "legendFormat": "Hours Since Last Training",
            "refId": "C"
          }
        ]
      }
    ],
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "model",
          "type": "query",
          "query": "label_values(trading_bot_ml_model_accuracy, model)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {"list": []},
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}